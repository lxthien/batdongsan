<IfModule mod_rewrite.c>

Options +SymLinksIfOwnerMatch
RewriteEngine on
RewriteBase /
RewriteCond %{HTTP_HOST} ^www.sotaynhadat.vn [NC]
RewriteRule ^(.*)$ http://sotaynhadat.vn/$1 [L,R=301]

RewriteRule ^cam-nang/doanh-nghiep(.*)$ http://sotaynhadat.vn/doanh-nghiep/tin-doanh-nghiep$1 [R=301,L]
RewriteRule ^du-an/khu-biet-thu-do-thi-moi(.*)$ http://sotaynhadat.vn/du-an/khu-do-thi-moi$1 [R=301,L]
RewriteRule ^cam-nang/the-gioi-doanh-nhan(.*)$ http://sotaynhadat.vn/doanh-nghiep/the-gioi-doanh-nhan$1 [R=301,L]
RewriteRule ^du-an/khu-to-hop-thuong-mai(.*)$ http://sotaynhadat.vn/du-an/khu-du-lich-nghi-duong$1 [R=301,L]
RewriteRule ^dang-tin-mien-phi http://sotaynhadat.vn/dang-tin-rao-vat-nha-dat-mien-phi [R=301,L]

RewriteRule ^tin-tuc/tin-thi-truong/dich-vu-xay-dung-va-sua-chua-nha-o-uy-tin-tai-vung-tau.html http://sotaynhadat.vn/tin-tuc/tin-thi-truong/dich-vu-xay-dung-sua-chua-nha-o-uy-tin-tai-vung-tau.html [R=301,L]
RewriteRule ^du-an/can-ho-chung-cu/bau-sen-building.html http://sotaynhadat.vn/du-an/can-ho-chung-cu/melody-vung-tau.html [R=301,L]

RewriteRule ^nhu-cau-nha-dat/hop-tac-dau-tu http://sotaynhadat.vn/ [R=301,L]
RewriteRule ^nhu-cau-nha-dat/can-mua http://sotaynhadat.vn/ [R=301,L]
RewriteRule ^nhu-cau-nha-dat/can-thue http://sotaynhadat.vn/ [R=301,L]
RewriteRule ^nha-dat-cho-thue/cho-thue-nha-xuong-kho-bai http://sotaynhadat.vn/nha-dat-cho-thue/cho-thue-dat-xuong-kho-bai [R=301,L]

RewriteRule ^nha-dat-ban/ban-biet-thu-lien-ke http://sotaynhadat.vn/nha-dat-ban/ban-biet-thu-nha-lien-ke [R=301,L]
RewriteRule ^cho-thue-dat/tp-vung-tau/ba-ria-vung-tau http://sotaynhadat.vn/cho-thue-dat-xuong-kho-bai/tp-vung-tau/ba-ria-vung-tau [R=301,L]
RewriteRule ^cho-thue-nha-xuong-kho-bai/tp-vung-tau/ba-ria-vung-tau http://sotaynhadat.vn/cho-thue-dat-xuong-kho-bai/tp-vung-tau/ba-ria-vung-tau [R=301,L]

<ifModule mod_headers.c>
ExpiresActive On
# Expires after 1 month
<filesMatch ".(gif|png|jpg|jpeg|ico|pdf|js|htm|html)$">
Header set Cache-Control "max-age=2592000"
</filesMatch>
# Expires after 7 day
<filesMatch ".(css)$">
Header set Cache-Control "max-age=604800"
</filesMatch>
# Expires after 1 hour
<filesMatch ".(txt)$">
Header set Cache-Control "max-age=3600"
</filesMatch>
</ifModule>

# compress text, HTML, JavaScript, CSS, and XML
AddOutputFilterByType DEFLATE text/plain
AddOutputFilterByType DEFLATE text/html
AddOutputFilterByType DEFLATE text/xml
AddOutputFilterByType DEFLATE text/css
AddOutputFilterByType DEFLATE application/xml
AddOutputFilterByType DEFLATE application/xhtml+xml
AddOutputFilterByType DEFLATE application/rss+xml
AddOutputFilterByType DEFLATE application/javascript
AddOutputFilterByType DEFLATE application/x-javascript
# remove browser bugs
BrowserMatch ^Mozilla/4 gzip-only-text/html
BrowserMatch ^Mozilla/4\.0[678] no-gzip
BrowserMatch \bMSIE !no-gzip !gzip-only-text/html
Header append Vary User-Agent

#RewriteRule ^(.*)/$ /$1 [L,R=301]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?/$1 [QSA,L]
<FilesMatch "\.(htaccess|htpasswd|ini|phps|fla|psd|log|sh)$">
     Order Allow,Deny
     Deny from all
</FilesMatch>
</ifModule>
